<div class="page-header">
	<h1>Bobak</h1>
</div>
<div>
	<form class="form-search">
	  <input type="text" name="q" class="input search-query" value="<%= params[:q]%>">
	  <button type="submit" class="btn">Search</button>
	</form>
</div>
<div>
<table class="table">
	<tr>
		<th>Jedinstveni matični broj</th>
		<th>Naziv vlasnika</th>
		<th>Broj blokiranih računa</th>
	</tr>
	<% @owners.each do |owner| %>
	<tr>
		<td><%= owner.oid %></td>
		<td><%= owner.name %></td>
		<td><%= owner.accounts.size %></td>
	</tr>
	<% end if @owners %>
</table>
</div>