<script type="text/javascript" src="https://www.google.com/jsapi"></script>
<script type="text/javascript">
  google.load("visualization", "1", {packages:["corechart"]});
  google.setOnLoadCallback(drawChart);

  function drawChart() {

    var options = {};
    var report_data = <%= raw @report %>;
    
    var data = new google.visualization.DataTable(report_data);
    data.sort({column: 1, desc: true});

    var chart = new google.visualization.ColumnChart(document.getElementById('chart_div')).draw(data, options);
  }
</script>
<div id="mainContainer">
    <div class="innerHolder">
       <div id="chart_div" style="width: 715px; height: 550px;"></div>
    </div>
  </div>
</div>